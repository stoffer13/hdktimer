{
  "name": "HDK timer",
  "version": "1.0",
  "description": "Times .hdk pages",
  "permissions": ["storage", "webNavigation", "*://*.hdk/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
